{
  "name": "vue-dark-switch",
  "version": "1.0.0",
  "description": "多合一的开箱即用 vue3 暗黑模式开关组件",
  "main": "./dist/exports.cjs",
  "module": "./dist/exports.mjs",
  "types": "./dist/exports.d.ts",
  "exports": {
    ".": {
      "require": "./dist/exports.cjs",
      "import": "./dist/exports.mjs",
      "types": "./dist/exports.d.ts"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs",
      "types": "./dist/vite.d.ts"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "dev": "vite",
    "build-js": "vite build",
    "prepublishOnly": "pnpm build:cached",
    "build-types": "vue-tsc -p tsconfig.build.json",
    "play": "pnpm -C examples/vite-project run dev",
    "copy-polyfill": "fast-cpy src/polyfill dist/polyfill",
    "build:cached": "turbo build --filter=vue-dark-switch",
    "release": "bumpp --commit --push --tag && npm publish",
    "play:build": "pnpm -C examples/vite-project run build",
    "play:preview": "pnpm -C examples/vite-project run preview",
    "build": "rimraf -rf dist && run-p copy-polyfill build-js build-types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dishait/vue-dark-switch.git"
  },
  "keywords": [
    "switch",
    "dark",
    "theme"
  ],
  "license": "MIT",
  "author": {
    "name": "markthree",
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.24",
    "@types/node": "^18.16.14",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^9.13.0",
    "bumpp": "^9.1.0",
    "fast-cpy": "^0.0.8",
    "naive-ui": "^2.34.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "turbo": "^1.9.9",
    "typescript": "^5.0.4",
    "unocss": "^0.52.3",
    "vite": "^4.3.8",
    "vite-layers": "^0.3.5",
    "vite-plugin-lib-inject-css": "^1.2.0",
    "vite-plugin-warmup": "^0.1.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  },
  "peerDependencies": {
    "vite": ">=4.0.0",
    "vue": ">=3.0.0"
  }
}
